<% layout('layout', { title:'Sign Up' }) %>
<h2>Create an account</h2>
<form action="/signup" method="get">
  <label>Role:</label>
  <select name="role">
    <option value="Client">Client</option>
    <option value="Freelancer">Freelancer</option>
  </select>
  <button type="submit">Continue with Google</button>
</form>
